{
  "name": "@stickyculture/lotion",
  "version": "2.1.0",
  "description": "copy a Notion database to a local place",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "cli": "echo '#!/usr/bin/env node' | cat - dist/cli.js > temp && mv temp dist/cli.js",
    "build": "tsc -p tsconfig.json && yarn run cli",
    "docs": "typedoc --readme none --cleanOutputDir --out docs --plugin typedoc-plugin-markdown --entryPointStrategy expand ./src/index.ts ./src/types.ts",
    "test": "jest",
    "test:js": "yarn run build && node dist/index.js",
    "test:cli": "yarn run build && node dist/cli.js",
    "prepare": "yarn build && husky"
  },
  "bin": {
    "lotion": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "author": "Justin Seibert",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.12",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "colorette": "^2.0.20",
    "commander": "^11.0.0",
    "cosmiconfig": "^8.3.4",
    "dotenv": "^16.3.1",
    "image-size": "^1.0.2"
  }
}
