{
  "name": "sticky-utils-lotion",
  "version": "1.4.0",
  "description": "copy a Notion database to a local place",
  "main": "src/index.ts",
  "scripts": {
    "cli": "cp dist/index.js dist/cli.js && echo '#!/usr/bin/env node' | cat - dist/cli.js > temp && mv temp dist/cli.js",
    "build": "tsc -p tsconfig.json && yarn run cli",
    "test": "yarn run build && node dist/index.js",
    "prepare": "yarn run build"
  },
  "bin": {
    "sticky-utils-lotion": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "author": "Justin Seibert",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "calipers": "^3.0.0",
    "calipers-jpeg": "^3.0.0",
    "calipers-png": "^3.0.0",
    "colorette": "^2.0.20",
    "cosmiconfig": "^8.3.4",
    "dotenv": "^16.3.1"
  }
}
